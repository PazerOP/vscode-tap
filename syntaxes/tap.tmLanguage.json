{
    "comment": "Test Anything Protocol Syntax: version 13",
    "fileTypes": ["tap"],
    "name": "Tap",
    "patterns": [{
        "begin": "^\\s+---$",
        "end": "^\\s+\\.\\.\\.$",
        "patterns": [{
            "include": "source.yaml"
        }]
    }, {
        "match": "#.*$",
        "name": "comment.line.number-sign.tap"
    }, {
        "match": "^TAP version \\d+$",
        "name": "markup.heading"
    }, {
        "match": "^\\s*\\d+\\.\\.\\d+$",
        "name": "storage.modifier"
    }, {
        "match": "^\\s*(ok|not ok)\\s+(\\d+)(?:\\s+(-)?\\s*([^#\\n]*))?",
        "captures": {
            "1": {"name": "storage.type"},
            "2": {"name": "constant.numeric"},
            "4": {"name": "string.unquoted"}
        }
    }],
    "scopeName": "source.tap"
}
